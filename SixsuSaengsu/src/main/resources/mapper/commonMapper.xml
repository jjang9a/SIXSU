<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="co.sixsu.app.mapper.CommonMapper">

<select id="getDate" resultType="java.util.Date">
	select sysdate from dual
</select>

<select id="empList" resultType="co.sixsu.app.domain.common.EmpVO">
	select emp_id, emp_name, emp_dept from employee
</select>

</mapper>